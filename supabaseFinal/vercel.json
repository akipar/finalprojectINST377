{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
      "src": "/page_views",
      "dest": "/api/index.js"
    },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}